---
const myAPI = Astro.locals.runtime.env.SECRET_NOCOAPI;
const myTOK = Astro.locals.runtime.env.SECRET_NOCO;
---

<script>
const response = await fetch(`"${myAPI}`,
  {
    method: 'POST',
    headers: {
    'xc-token': `'${myTOK}'`
    },
  });

const json = await response.json();
console.log(json);
</script>
