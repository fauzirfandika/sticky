---
import Layout from '../../../../layouts/layout.astro';
const { undangan } = Astro.params;
const { alamat } = Astro.params;
const { daerah } = Astro.params;
---
<Layout>
  <Fragment slot="slot-undangan">
    {undangan}
  </Fragment>
  <Fragment slot="slot-alamat">
    {alamat}
  </Fragment>
  <Fragment slot="slot-daerah">
    {daerah}
  </Fragment>
  <Fragment slot="slot-waktu">
    18.30 - 21.00
  </Fragment>
</Layout>
