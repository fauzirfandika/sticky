---
import Layout from '../layouts/layout.astro'
import Count from '../scripts/ctds.astro'
const myAPI = Astro.locals.runtime.env.SECRET_NOCOAPI;
const myTOK = Astro.locals.runtime.env.SECRET_NOCO;

const response = await fetch(myAPI,
  {
    method: 'GET',
    headers: {
    'xc-token': myTOK
    },
  });

const json = await response.json();
const nama = json.list[0].namaundangan;
---
<Layout>
  <Fragment slot="namaundangan">
    {nama}
  </Fragment>
  <Fragment slot="wtu">
    13:00 - 21:00
  </Fragment>
</Layout>
<Count/>
<script src="../scripts/fecth.js"></script>
